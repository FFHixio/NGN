{
  "name": "build",
  "version": "1.0.0",
  "description": "Build Utility",
  "main": "index.js",
  "scripts": {
    "start": "rollup -c rollup.config.js && node ./lib/package.js",
    "test": "echo \"See the test directory for library testing utilities.\"",
    "test:node": "rollup -c rollup.test.node.config.js",
    "test:browser": "rollup -c rollup.test.browser.config.js",
    "all": "npm run clean && npm run browser && npm run node",
    "browser": "rollup -c rollup.browser.config.js && node ./lib/browser-package.js",
    "clean": "cd ../ && node ./build/lib/reset.js",
    "test:browser:core": "rollup -c ./runtime/browser/component.test.js -- ../src/core/index.js",
    "test:node:core": "rollup -c ./runtime/node/component.test.js -- ../src/core/index.js"
  },
  "author": "",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@author.io/metadoc": "0.1.2",
    "@author.io/metadoc-api": "1.1.0",
    "@author.io/metadoc-md": "0.0.5",
    "@babel/core": "^7.10.2",
    "@babel/plugin-external-helpers": "^7.10.1",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-private-methods": "^7.10.1",
    "@babel/plugin-transform-flow-strip-types": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@externs/nodejs": "^1.5.0",
    "@rollup/plugin-multi-entry": "^3.0.1",
    "browserslist": "^4.12.0",
    "chalk": "^4.0.0",
    "core-js": "^3.6.5",
    "rollup": "^2.12.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-strip-code": "^0.2.7",
    "rollup-plugin-terser": "^5.3.0",
    "shortbus": "1.2.8",
    "source-map-support": "^0.5.19"
  }
}

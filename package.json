{
  "name": "ngn",
  "version": "2.0.0",
  "description": "A JavaScript-based systems and software authoring library.",
  "scripts": {
    "test": "gulp test",
    "test:build": "gulp test:clean && gulp test:build",
    "xtest:node": "npm run test:build && nyc tape ./test/common/*.js --coverage | tap-spec && standard --verbose | tap-nyc | snazzy && npm run cover",
    "test:browser": "npm run test:build && karma start karma.conf.js ",
    "test:browser:data": "npm run test:build && karma start karma.conf.js --test data",
    "test:browser:events": "npm run test:build && karma start karma.conf.js --test eventemitter",
    "test:browser:utility": "npm run test:build && karma start karma.conf.js --test utility",
    "coverage": "nyc tape ./test/common/*.js | tap-nyc",
    "coverage:report": "nyc report --reporter=html",
    "cover": "npm run coverage && npm run coverage:report",
    "test:build:2": "node build.js --test",
    "test:node": "rollup -c ./rollup.testnode.config.js && tap -R spec ./test/common/*-**/*-*.js --cov --coverage-report=html && standard --verbose | snazzy",
    "test:node:sanity": "rollup -c ./rollup.testnode.config.js && tap -R spec ./test/common/*-sanity/**/*.js && standard --verbose | snazzy",
    "test:node:basic": "rollup -c ./rollup.testnode.config.js && tap -R spec ./test/common/*-basic/**/*.js && standard --verbose | snazzy",
    "test:node:events": "rollup -c ./rollup.testnode.config.js && tap -R spec ./test/common/*-event/**/**.js && standard --verbose | snazzy",
    "test:node:utility": "rollup -c ./rollup.testnode.config.js && tap -R spec ./test/common/*-utility/**/*.js && standard --verbose | snazzy",
    "test:node:queue": "rollup -c ./rollup.testnode.config.js && tap -R spec ./test/common/*-queue/**/*.js && standard --verbose | snazzy",
    "test:node:data": "rollup -c ./rollup.testnode.config.js && tap -R spec ./test/common/*-data/**/*.js && standard --verbose | snazzy",
    "test:node:data:rules": "rollup -c ./rollup.testnode.config.js && tap -R spec ./test/common/*-data/*-rules.js && standard --verbose | snazzy",
    "test:node:data:schema": "rollup -c ./rollup.testnode.config.js && tap -R spec ./test/common/*-data/*-schema.js && standard --verbose | snazzy",
    "test:node:net": "rollup -c ./rollup.testnode.config.js && tap -R spec ./test/common/*-net/**/*.js && standard --verbose | snazzy",
    "test:node:integration": "rollup -c ./rollup.testnode.config.js && tap -R spec ./test/common/*-integration/**/*.js && standard --verbose | snazzy",
    "build": "rollup -c ./rollup.release.config.js",
    "documentation": "rollup -c ./rollup.documentation.config.js && node build.js --docs",
    "metadoc": "metadoc --source ./src --output ./docs --warnskippedevents --warnnocode --ignore ./src/data/WIP | metadoc-md",
    "md": "metadoc --source ./src --output ./docs --warnskippedevents --warnnocode --ignore ./src/data/WIP | metadoc-api | metadoc-md",
    "md2": "metadoc --source ./src --output ./docs --warnskippedevents --warnnocode --ignore ./src/data/WIP | metadoc-md | metadoc-api",
    "blag": " | metadoc-api | metadoc-md",
    "docs": "metadoc --source ./src --output ./docs --warnskippedevents --warnnocode --ignore ./src/data/WIP | metadoc-api"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ngnjs/ngn.git"
  },
  "keywords": [
    "chassis",
    "js",
    "dom",
    "http",
    "ajax",
    "http",
    "references",
    "ref",
    "svg",
    "data",
    "store",
    "collection",
    "bus",
    "event",
    "bus",
    "NGN",
    "ngn",
    "orchestration"
  ],
  "author": {
    "name": "Author.io",
    "url": "https://author.io"
  },
  "contributors": [
    {
      "name": "Corey Butler",
      "url": "http://coreybutler.com"
    },
    {
      "name": "Graham Butler",
      "url": "http://grahambutler.com"
    },
    {
      "name": "Kevin Moritz",
      "url": "https://github.com/mayorbyrne"
    },
    {
      "name": "TJ Loughry",
      "url": "https://github.com/theteejers"
    }
  ],
  "license": "BSD-3-Clause",
  "homepage": "https://ngn.js.org",
  "devDependencies": {
    "@author.io/metadoc": "^0.0.9",
    "@author.io/metadoc-api": "^1.0.4",
    "@author.io/metadoc-md": "^0.0.3",
    "browserify": "^14.5.0",
    "browserify-istanbul": "^3.0.1",
    "browserslist": "^2.5.1",
    "buffer-equal-constant-time": "1.0.1",
    "chalk": "^2.4.1",
    "del": "^3.0.0",
    "fs-extra": "^4.0.2",
    "karma": "^3.0.0",
    "karma-browserify": "^5.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-opera-launcher": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-safari-launcher": "^1.0.0",
    "karma-sauce-launcher": "^1.2.0",
    "karma-spec-reporter": "0.0.31",
    "karma-tap": "^3.2.1",
    "karma-tap-pretty-reporter": "^3.0.5",
    "localenvironment": "^1.0.2",
    "minimatch": "^3.0.4",
    "musthave": "^1.0.2",
    "phantomjs-prebuilt": "^2.1.16",
    "productionline": "1.3.8",
    "rollup": "^0.59.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-strip-code": "^0.2.6",
    "rollup-plugin-terser": "^1.0.1",
    "rollup-plugin-uglify": "^4.0.0",
    "shortbus": "^1.2.8",
    "snazzy": "^7.1.1",
    "source-map-support": "^0.5.9",
    "standard": "^11.0.1",
    "table": "^4.0.3",
    "tap": "^12.0.1",
    "tape": "^4.9.1"
  },
  "standard": {
    "ignore": [
      "_*",
      "_**/*",
      "test/lib/**/*",
      "node_modules",
      "dist/**/*",
      "gulpfile.js",
      "src/common/data/WIP/**/*",
      "src/common/data/current/**/*",
      "rollup*.js",
      "karma.conf.js",
      "build.js"
    ],
    "globals": [
      "NGN",
      "NGNX",
      "window",
      "global",
      "MutationObserver",
      "EventEmitter",
      "NgnCustomException",
      "MissingNgnDependencyError",
      "InvalidConfigurationError",
      "ReservedWordError",
      "NGNDuplicateRecordError",
      "DOMParser",
      "XMLHttpRequest",
      "FormData",
      "btoa",
      "localStorage",
      "Network",
      "Request",
      "Response",
      "Headers",
      "fetch",
      "Element",
      "HTMLElement",
      "NodeFilter",
      "hostname",
      "networkInterfaces",
      "normalizeUrl",
      "HttpMethods",
      "request"
    ]
  },
  "nyc": {
    "exclude": [
      "test/common"
    ],
    "include": [
      "test/lib"
    ]
  },
